void loop() {
  // put your main code here, to run repeatedly:
  b = a;
  a = digitalRead(13);
  if (b == 1 && a == 0) {
    p = p == 1 ? 0 : 1;
  }
  if (p == 0) {
    Blink();
  }
  else {
    tang();
  }
  delay(20);
}
